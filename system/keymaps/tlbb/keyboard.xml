      <yellow>ActivateWindow(Favourites)</yellow>
    </keyboard>
  </MyMusicLibrary>
  <MusicSettings>
    <keyboard name="The Little Black Box Remote">
      <settings>Back</settings>
    </keyboard>
  </MusicSettings>
 <FullscreenVideo>
    <keyboard name="The Little Black Box Remote">
      <fastforward>FastForward</fastforward>
      <f3>Rewind</f3>
      <backspace>Fullscreen</backspace>
      <select>OSD</select>
      <menu>OSD</menu>
      <help>Info</help>
      <left>StepBack</left>
      <right>StepForward</right>
      <up>BigStepForward</up>
      <down>BigStepBack</down>
      <music>AudioDelay</music>
      <f2>AspectRatio</f2> 
      <red>RunScript(script.xbmc.subtitles)</red>
      <green>ActivateWindow(OSDVideoSettings)</green>
      <yellow>ActivateWindow(OSDAudioSettings)</yellow>
      <blue>ActivateWindow(VideoBookmarks)</blue>
      <f1>ActivateWindow(VideoPlaylist)</f1>
      <f7>SkipNext</f7>
      <rewind>SkipPrevious</rewind>
    </keyboard>
  </FullscreenVideo>
  <VideoTimeSeek>
    <keyboard name="The Little Black Box Remote">
