<!-- Mappings for The Little Black Box Remote -->
<keymap>
  <global>
    <keyboard name="The Little Black Box Remote">
      <!-- <>power</>-->										<!-- Button 01 : Power -->
      <f10>Mute</f10>										<!-- Button 02 : Mute -->
      <rewind>SkipPrevious</rewind>							<!-- Button 03 : |< -->
      <play_pause>Play</play_pause>								<!-- Button 04 : ||> -->
      <f7>SkipNext</f7>								<!-- Button 05 : >| -->
      <f9>ActivateWindow(accesspoints)</f9>							<!-- Button 06 : Wifi -->
      <f3>Rewind</f3>									<!-- Button 07 : << -->
      <f8>Stop</f8>										<!-- Button 08 : stop -->
      <fastforward>FastForward</fastforward>							<!-- Button 09 : >> -->
      <menu>ContextMenu</menu>									<!-- Button 10 : menu -->
      <!-- <>mouse on/off</> -->									<!-- Button 11 : mouse (not available for keycode) -->
      <settings>ActivateWindow(Settings)</settings>						<!-- Button 12 : settings/wheel -->
      <backspace>Back</backspace>									<!-- Button 13 : back -->
      <up>Up</up>										<!-- Button 14 : up -->
      <left>Left</left>										<!-- Button 15 : left -->
      <select>Select</select>									<!-- Button 16 : OK -->
      <right>Right</right>									<!-- Button 17 : right -->
      <down>Down</down>										<!-- Button 18 : down -->
      <f12>VolumeUp</f12>								<!-- Button 19 : vol+ -->
      <f11>VolumeDown</f11>							<!-- Button 20 : vol- -->
      <pageup>PageUp</pageup>  								<!-- Button 21 : pg+ -->
      <pagedown>PageDown</pagedown>								<!-- Button 22 : pg- -->
      <f4>ActivateWindow(Home)</f4>							<!-- Button 23 : home -->
      <f1>ContextMenu</f1>									<!-- Button 24 : = -->
      <one>Number1</one>										<!-- Button 25 : 1 -->
      <two>Number2</two>										<!-- Button 26 : 2 -->
      <three>Number3</three>										<!-- Button 27 : 3 -->
      <music>ActivateWindow(Music)</music>							<!-- Button 28 : music -->
      <four>Number4</four>										<!-- Button 29 : 4 -->
      <five>Number5</five>										<!-- Button 30 : 5 -->
      <six>Number6</six>										<!-- Button 31 : 6 -->
      <movies>ActivateWindow(Videos,MovieTitles)</movies>						     	<!-- Button 32 : movies -->
      <seven>Number7</seven>										<!-- Button 33 : 7 -->
      <eight>Number8</eight>										<!-- Button 34 : 8 -->
      <nine>Number9</nine>										<!-- Button 35 : 9 -->
      <series>ActivateWindow(Videos,TvShowTitles)</series>							<!-- Button 36 : series -->
      <help>Info</help>										<!-- Button 37 : info -->
      <zero>Number0</zero>										<!-- Button 38 : 0 -->
      <backspace>Back</backspace>										<!-- Button 39 : back -->
      <f2>ActivateWindow(Pictures)</f2> 						<!-- Button 40 : pictures -->
      <red>Close</red>  										<!-- Button 41 : red -->
      <green>Close</green>									<!-- Button 42 : green -->
      <yellow>Close</yellow>						<!-- Button 43 : yellow -->
      <blue>FullScreen</blue>										<!-- Button 44 : blue -->
    </keyboard>
  </global>
  <LoginScreen>
    <keyboard name="The Little Black Box Remote">
      <!-- Empty -->
    </keyboard>
  </LoginScreen>
  <Home>
    <keyboard name="The Little Black Box Remote">
      <help>Info</help>
      <f4>FullScreen</f4>
      <f1>Playlist</f1>
      <zero>Playlist</zero>
      <yellow>ActivateWindow(Favourites)</yellow>
    </keyboard>
  </Home>
  <Programs>
    <keyboard name="The Little Black Box Remote">
      <yellow>ActivateWindow(Favourites)</yellow>
    </keyboard>
  </Programs>
  <Settings>
    <keyboard name="The Little Black Box Remote">
      <settings>Back</settings>
      <yellow>ActivateWindow(Favourites)</yellow>
    </keyboard>
  </Settings>
  <VirtualKeyboard>
    <keyboard name="The Little Black Box Remote">
      <backspace>Backspace</backspace> 		 					<!-- Keyboard -->
    </keyboard>
  </VirtualKeyboard>
  <MyFiles>
