  <PlayerControls>
    <keyboard name="The Little Black Box Remote">
      <menu>Back</menu>
    </keyboard>
  </PlayerControls>
  <Visualisation>
    <keyboard name="The Little Black Box Remote">
      <c>RunScript(special://xbmc/system/keymaps/tlbb/py_ispvr.py,"ActivateWindow(PVROSDChannels)","ActivateWindow(MusicPlaylist)")</c>
      <fastforward>FastForward</fastforward>
      <f3>Rewind</f3>
      <backspace>Fullscreen</backspace>
      <select>OSD</select>
      <menu>OSD</menu>
      <help>Info</help>
      <left>SkipPrevious</left>
      <right>SkipNext</right>
      <up>RunScript(special://xbmc/system/keymaps/nyxboard/py_ispvr.py,"ActivateWindow(PVROSDChannels)","Action(IncreaseRating)")</up>
      <down>RunScript(special://xbmc/system/keymaps/nyxboard/py_ispvr.py,"ActivateWindow(PVROSDChannels)","Action(DecreaseRating)")</down>
      <red>RunScript(script.cu.lrclyrics)</red>
      <green>RunScript(special://xbmc/system/keymaps/tlbb/py_activate_click.py,500)</green>
      <yellow>Addon.Default.OpenSettings(xbmc.player.musicviz)</yellow>
      <blue>ActivateWindow(VisualisationPresetList)</blue>
      <f1>ActivateWindow(MusicPlaylist)</f1>
    </keyboard>
  </Visualisation>
  <MusicOSD>
    <keyboard name="The Little Black Box Remote">
      <fastforward>FastForward</fastforward>
      <f3>Rewind</f3>
      <f7>SkipNext</f7>
      <rewind>SkipPrevious</rewind>
      <menu>Back</menu>
      <help>Info</help>
      <red>RunScript(script.cu.lrclyrics)</red>
      <green>SendClick(500)</green>
      <yellow>Addon.Default.OpenSettings(xbmc.player.musicviz)</yellow>
      <blue>ActivateWindow(VisualisationPresetList)</blue>
      <f1>ActivateWindow(MusicPlaylist)</f1>
    </keyboard>
  </MusicOSD>
