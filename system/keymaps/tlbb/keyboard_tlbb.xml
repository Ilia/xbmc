<!-- Mappings for CDRC RF091 based on the Motorola Nyxboard remote -->
<keymap>
  <global>
    <keyboard name="Motorola Nyxboard Hybrid">
      <!-- <>power</>-->										<!-- Button 01 : Power -->
      <f10>Mute</f10>										<!-- Button 02 : Mute -->
      <media_rewind>SkipPrevious</media_rewind>							<!-- Button 03 : |< -->
      <browser_forward>Play</browser_forward>								<!-- Button 04 : ||> -->
      <f7>SkipNext</f7>								<!-- Button 05 : >| -->
      <f9>ActivateWindow(accesspoints)</f9>							<!-- Button 06 : Wifi -->
      <f3>Rewind</f3>									<!-- Button 07 : << -->
      <f8>Stop</f8>										<!-- Button 08 : stop -->
      <fastforward_btn>FastForward</fastforward_btn>							<!-- Button 09 : >> -->
      <menu_btn>ContextMenu</menu_btn>									<!-- Button 10 : menu -->
      <!-- <>mouse on/off</> -->									<!-- Button 11 : mouse (not available for keycode) -->
      <settings_btn>ActivateWindow(Settings)</settings_btn>						<!-- Button 12 : settings/wheel -->
      <backspace>Back</backspace>									<!-- Button 13 : back -->
      <up_btn>Up</up_btn>										<!-- Button 14 : up -->
      <left_btn>Left</left_btn>										<!-- Button 15 : left -->
      <select_btn>Select</select_btn>									<!-- Button 16 : OK -->
      <right_btn>Right</right_btn>									<!-- Button 17 : right -->
      <down_btn>Down</down_btn>										<!-- Button 18 : down -->
      <f12>VolumeUp</f12>								<!-- Button 19 : vol+ -->
      <f11>VolumeDown</f11>							<!-- Button 20 : vol- -->
      <pageup>PageUp</pageup>  								<!-- Button 21 : pg+ -->
      <pagedown>PageDown</pagedown>								<!-- Button 22 : pg- -->
      <f4>ActivateWindow(Home)</f4>							<!-- Button 23 : home -->
      <f10>ContextMenu</f10>									<!-- Button 24 : = -->
      <one>Number1</one>										<!-- Button 25 : 1 -->
      <two>Number2</two>										<!-- Button 26 : 2 -->
      <three>Number3</three>										<!-- Button 27 : 3 -->
      <music_btn>ActivateWindow(Music)</music_btn>							<!-- Button 28 : music -->
      <four>Number4</four>										<!-- Button 29 : 4 -->
      <five>Number5</five>										<!-- Button 30 : 5 -->
      <six>Number6</six>										<!-- Button 31 : 6 -->
      <movies_btn>ActivateWindow(Videos,MovieTitles)</movies_btn>						     	<!-- Button 32 : movies -->
      <seven>Number7</seven>										<!-- Button 33 : 7 -->
      <eight>Number8</eight>										<!-- Button 34 : 8 -->
      <nine>Number9</nine>										<!-- Button 35 : 9 -->
      <series_btn>ActivateWindow(Videos,TvShowTitles)</series_btn>							<!-- Button 36 : series -->
      <info_btn>Info</info_btn>										<!-- Button 37 : info -->
      <zero>Number0</zero>										<!-- Button 38 : 0 -->
      <backspace>Back</backspace>										<!-- Button 39 : back -->
      <f2>ActivateWindow(Pictures)</f2> 						<!-- Button 40 : pictures -->
      <red_btn>Close</red_btn>  										<!-- Button 41 : red -->
      <green_btn>Close</green_btn>									<!-- Button 42 : green -->
      <yellow_btn>Close</yellow_btn>						<!-- Button 43 : yellow -->
      <blue_btn>FullScreen</blue_btn>										<!-- Button 44 : blue -->
    </keyboard>
  </global>
